<?xml version="1.0" encoding="UTF-8"?>
<project name="kernet-rules" default="help">

  <!-- A rule to generate the JAR for inclusion in an Android
       application. Output file will be bin/kernet.jar -->
  <target name="jar" depends="-compile">
    <jar destfile="bin/kernet.jar"
         basedir="bin/classes" />
  </target>
</project>
